{
  "name": "NetDaemon",
  "version": "0.11.0",
  "slug": "netdaemon",
  "description": "Application daemon written in .NET core 3.1",
  "url": "http://netdaemon.xyz",
  "startup": "application",
  "image": "netdaemon/netdaemon",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7"
  ],
  "boot": "auto",
  "map": [
    "config:rw"
  ],
  "ports": {
    "1337/tcp": 1337
  },
  "hassio_api": true,
  "hassio_role": "default",
  "homeassistant_api": true,
  "host_network": false,
  "options": {
    "log_level": "info",
    "log_messages": false,
    "generate_entities": false,
    "project_folder": ""
  },
  "schema": {
    "log_level": "match(^(trace|debug|info|warning|error)$)",
    "log_messages": "bool?",
    "generate_entities": "bool?"
  }
}

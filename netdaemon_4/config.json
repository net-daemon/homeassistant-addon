    "mqtt_client_id": "str?",
    "mqtt_discoveryprefix": "str",
    "mqtt_use_tls": "bool",
    "mqtt_allow_untrusted_certificates": "bool",
    "mqtt_ignore_cert_validation_errors": "bool",
    "mqtt_ignore_certificate_chain_errors": "bool"
  },
  "discovery": [
    "mqtt"
  ],
  "services": [
    "mqtt:want"

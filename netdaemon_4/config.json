  "ports": {
    "10000/tcp": null,
    "10001/tcp": null,
    "10002/tcp": null,
    "10003/tcp": null,
    "10004/tcp": null,
    "20000/udp": null,
    "20001/udp": null,
    "20002/udp": null,
    "20003/udp": null,
    "20004/udp": null

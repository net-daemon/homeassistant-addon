  "startup": "application",
  "image": "ghcr.io/net-daemon/netdaemon_addon4",
  "init": false,
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7"
  ],
  "boot": "auto",
  "map": [
    "config:rw"
  ],
  "ingress": false,
  "hassio_api": true,
  "hassio_role": "default",
  "homeassistant_api": true,
  "host_network": false,
  "options": {
    "log_level": "information",
    "app_config_folder": "/config/netdaemon4",
    "app_assembly": "",
    "mqtt_host": null,
    "mqtt_port": null,
    "mqtt_username": null,
    "mqtt_password": null,
    "mqtt_client_id": null,
    "mqtt_discoveryprefix": "homeassistant",

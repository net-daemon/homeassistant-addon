{
  "name": "NetDaemon V5 (.NET 9)",
  "version": "25.18.1",
  "slug": "netdaemon5",
  "description": "NetDaemon V5 (.NET 9), Write automations in C# for Home Assistant",
  "url": "https://netdaemon.xyz",
  "startup": "application",
  "image": "ghcr.io/net-daemon/netdaemon_addon5",
  "init": false,
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7"
  ],
  "boot": "auto",
  "map": [
    "config:rw"
  ],
  "ingress": false,
  "hassio_api": true,
  "hassio_role": "default",
  "homeassistant_api": true,
  "host_network": false,
  "options": {
    "log_level": "information",
    "app_config_folder": "/config/netdaemon5",
    "app_assembly": ""
  },
  "schema": {
    "log_level": "match(^(trace|debug|information|warning|error|critical)$)",
    "app_config_folder": "str",
    "app_assembly": "str?",
    "mqtt_host": "str?",
    "mqtt_port": "int(0,65535)?",
    "mqtt_username": "str?",
    "mqtt_password": "str?",
    "mqtt_discovery_prefix": "str?",
    "mqtt_use_tls": "bool?",
    "mqtt_allow_untrusted_certificates": "bool?"
  },
  "discovery": [
    "mqtt"
  ],
  "services": [
    "mqtt:want"
  ],
  "ports": {
    "10000/tcp": null,
    "10001/tcp": null,
    "10002/tcp": null,
    "10003/tcp": null,
    "10004/tcp": null,
    "20000/udp": null,
    "20001/udp": null,
    "20002/udp": null,
    "20003/udp": null,
    "20004/udp": null
  }
}

